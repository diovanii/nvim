snippet ah "a link"
<a href="https://" target="_blank">$1</a>
endsnippet

snippet art "article"
<article>
	$1
</article>
endsnippet

snippet bb "bold text"
<b>$1</b>
endsnippet

snippet body "body"
<body>
	$1
</body>
endsnippet

snippet br "break line"
<br/>$1
endsnippet

